{
	"name": "[nodata][audit][multi alert][below threshold][default]",
	"type": "audit alert",
	"query": "audits(\"GET request made to \\/api\\/v2\\/roles\\/\").rollup(\"count\").by(\"@http.url_details.path\").last(\"5m\") < 1",
	"message": "Audit monitor test QA\n \n@daljeet.sandu@datadoghq.com",
	"tags": [],
	"options": {
		"thresholds": {
			"critical": 1,
			"period": {
				"digit": 5,
				"name": "5 minute average",
				"no_data_timeframe": 10,
				"seconds": 300,
				"tense": "last",
				"text": "5 minutes",
				"unit": "minutes",
				"value": "last_5m"
			},
			"timeAggregator": "avg",
			"comparison": "<"
		},
		"enable_logs_sample": false,
		"notify_audit": false,
		"restriction_query": null,
		"on_missing_data": "default"
	}
}
