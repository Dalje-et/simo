{
	"name": "[nodata][audit][simple alert][above threshold][show_and_notify_no_data]",
	"type": "audit alert",
"query":"audits(\"GET request made to \\/api\\/v2\\/roles\\/\").rollup(\"count\").by(\"*\").last(\"5m\") > 1000",
	"message": "Audit monitor test QA\n \n@daljeet.sandu@datadoghq.com",
	"tags": [],
	"options": {
		"thresholds": {
			"critical": 1000,
			"period": {
				"digit": 0,
				"name": "5 minute average",
				"no_data_timeframe": 10,
				"seconds": 300,
				"tense": "last",
				"text": "5 minutes",
				"unit": "minutes",
				"value": "last_5m"
			},
			"timeAggregator": "avg",
			"comparison": ">"
		},
		"enable_logs_sample": false,
		"notify_audit": false,
		"restriction_query": null,
		"on_missing_data": "show_and_notify_no_data"
	}
}
