{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    "comma-dangle": 0,
    "object-curly-spacing": ["error", "always", { "arraysInObjects": false, "objectsInObjects": true }],
    "camelcase": 0,
    "max-params": 0,
    "padded-blocks": ["error", { "classes": "always", "switches": "never" }],
    "quotes": ["error", "double"]
  }
}
